# AI Provider API Keys
# Configure at least one of the following providers for code generation

# Gemini API (Default provider - Recommended)
# Get your API key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# OpenAI API (Optional)
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Together AI API (Optional)
# Get your API key from: https://api.together.xyz/settings/api-keys
TOGETHER_API_KEY=your_together_api_key_here

# OpenRouter API (Optional)
# Get your API key from: https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_api_key_here
 
# Mistral API (Optional)
# Get your API key from: https://console.mistral.ai/api-keys/
MISTRAL_API_KEY=your_mistral_api_key_here


# Database Configuration (Required if using PostgreSQL)
# If not set, will use in-memory storage
DATABASE_URL=postgresql://user:password@localhost:5432/neurocanvas

# Server Configuration
PORT=5000
NODE_ENV=development
